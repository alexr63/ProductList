<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="hotels" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xs:element name="hotels" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="hotel">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="hotel_ref" type="xs:string" minOccurs="0" />
              <xs:element name="hotel_name" type="xs:string" minOccurs="0" />
              <xs:element name="hotel_star" type="xs:string" minOccurs="0" />
              <xs:element name="hotel_address" type="xs:string" minOccurs="0" />
              <xs:element name="hotel_city" type="xs:string" minOccurs="0" />
              <xs:element name="hotel_pcode" type="xs:string" minOccurs="0" />
              <xs:element name="hotel_county" type="xs:string" minOccurs="0" />
              <xs:element name="hotel_country" type="xs:string" minOccurs="0" />
              <xs:element name="hotel_description" type="xs:string" minOccurs="0" />
              <xs:element name="alternate_description" type="xs:string" minOccurs="0" />
              <xs:element name="hotel_directions" type="xs:string" minOccurs="0" />
              <xs:element name="alternate_directions" type="xs:string" minOccurs="0" />
              <xs:element name="hotel_link" type="xs:string" minOccurs="0" />
              <xs:element name="customerrating" type="xs:string" minOccurs="0" />
              <xs:element name="PricesFrom" type="xs:string" minOccurs="0" />
              <xs:element name="MaxPrice" type="xs:string" minOccurs="0" />
              <xs:element name="CurrencyCode" type="xs:string" minOccurs="0" />
              <xs:element name="images" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="url" nillable="true" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:simpleContent msdata:ColumnName="url_Text" msdata:Ordinal="0">
                          <xs:extension base="xs:string">
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="geo_code" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="lat" type="xs:string" minOccurs="0" />
                    <xs:element name="long" type="xs:string" minOccurs="0" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="hotel_facilities" minOccurs="0" maxOccurs="unbounded">
			    <xs:complexType> 
					<xs:choice minOccurs="0" maxOccurs="unbounded"> 
						<xs:element name="id" type="xs:string"/> 
						<xs:element name="name" type="xs:string"/> 
					</xs:choice> 
				</xs:complexType> 
              </xs:element>
              <xs:element name="checkin_time" type="xs:string" minOccurs="0" />
              <xs:element name="checkout_time" type="xs:string" minOccurs="0" />
              <xs:element name="cancellationpolicy" type="xs:string" minOccurs="0" />
              <xs:element name="cancellationtext" type="xs:string" minOccurs="0" />
              <xs:element name="accommodation_type" type="xs:string" minOccurs="0" />
              <xs:element name="hotel_appeals" type="xs:string" minOccurs="0" />
              <xs:element name="hotel_star_accreditor" type="xs:string" minOccurs="0" />
              <xs:element name="hotel_total_rooms" type="xs:string" minOccurs="0" />
              <xs:element name="hotel_credit_cards" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="CreditCard" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="credit_card_id" type="xs:string" minOccurs="0" />
                          <xs:element name="credit_card_name" type="xs:string" minOccurs="0" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="hotel_city_taxes" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="CityTax" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Type" type="xs:string" minOccurs="0" />
                          <xs:element name="Value" type="xs:string" minOccurs="0" />
                          <xs:element name="OptedIn" type="xs:string" minOccurs="0" />
                          <xs:element name="IsCityTaxArea" type="xs:string" minOccurs="0" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>